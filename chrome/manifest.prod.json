{
  "version": "0.0.0",
  "name": "react-chrome-extension-example",
  "manifest_version": 2,
  "description": "Provides some features for Trello, related with the card activity",
  "browser_action": {
    "default_title": "React Chrome Extension Example",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
      "matches": ["https://trello.com/b/*"],
      "js": ["js/trello-aging.bundle.js"],
      "run_at": "document_start"
  }],
  "icons": {
    "64": "img/icon64.png",
    "128": "img/icon128.png",
    "256": "img/icon256.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [ "tabs", "storage", "https://trello.com/b/*" ]
}
